{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Too hipster to use react-create-app",
  "main": "index.js",
  "scripts": {
    "start-debug": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.development.js  --progress --colors",
    "start": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.development.js",
    "build": "NODE_ENV=production webpack --config config/webpack/webpack.production.js --progress --colors",
    "test": "NODE_ENV=test jest --coverage",
    "lint": "eslint --ext .jsx --ext .js --quiet . && prettier --check '**/*.{js,jsx,json,scss,html}'",
    "lint-fix": "eslint --ext .jsx --ext .js --quiet --fix . && prettier --write '**/*.{js,jsx,json,scss,html}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorfern91/webpack-react-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "victorfern91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorfern91/webpack-react-boilerplate/issues"
  },
  "homepage": "https://github.com/victorfern91/webpack-react-boilerplate#readme",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "error-overlay-webpack-plugin": "^0.1.7",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}

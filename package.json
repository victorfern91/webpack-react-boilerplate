{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Too hipster to use react-create-app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/webpack.development.js  --progress --colors",
    "start-dashboard": "webpack-dashboard -- webpack-dev-server --config config/webpack/webpack.development.js",
    "build": "NODE_ENV=production webpack --config config/webpack/webpack.production.js --progress --colors",
    "test": "jest --coverage",
    "lint": "eslint --ext .jsx --ext .js --quiet . && prettier --check '**/*.{js,jsx,json,scss,html}'",
    "fix-lint": "eslint --ext .jsx --ext .js --quiet --fix . && prettier --write '**/*.{js,jsx,json,scss,html}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorfern91/webpack-react-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "victorfern91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorfern91/webpack-react-boilerplate/issues"
  },
  "homepage": "https://github.com/victorfern91/webpack-react-boilerplate#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^2.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "error-overlay-webpack-plugin": "^0.1.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.4"
  }
}
